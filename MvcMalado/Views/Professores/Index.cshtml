@model IEnumerable<MvcMalado.Models.Professor>

<h2>Lista de Professores</h2>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nome</th>
            <th>Disciplina</th>
            <th>Anos de ExperiÃªncia</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var professor in Model)
